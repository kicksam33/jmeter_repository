          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">	int num=35544553;
int temp,sum=0,a;
temp=num;
while(num&gt;0)
{
	a=num%10;
	sum=(sum*10)+a;
	num=num/10;
}
if(temp==sum)
{
	log.info(&quot;number is a palindrome&quot;);
}
else
{
	log.info(&quot;number is not a palindrome&quot;);
}
</stringProp>
